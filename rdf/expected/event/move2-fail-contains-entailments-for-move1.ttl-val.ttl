[ a            sh:ValidationReport ;
  sh:conforms  false ;
  sh:result    [ a                             sh:ValidationResult ;
                 sh:focusNode                  ex:event1 ;
                 sh:resultMessage              "Event http://example.org/ns#event1 must set its https://w3id.org/valueflows#toResourceInventoriedAs resource urn:uuid:3585e2fc-3acc-4c3e-a366-6ce4f17c59a1 / https://w3id.org/valueflows#currentLocation to its value of property https://w3id.org/valueflows#atLocation, which would be http://www.wikidata.org/entity/Q1741 but the new value is urn:www.wikidata.org/entity/Q1741 ." ;
                 sh:resultPath                 vf:atLocation ;
                 sh:resultSeverity             sh:Violation ;
                 sh:sourceConstraintComponent  vfs:SameAsNextResourcePropertyConstraint ;
                 sh:sourceShape                [] 
               ] ;
  sh:result    [ a                             sh:ValidationResult ;
                 sh:focusNode                  ex:event1 ;
                 sh:resultMessage              "The only property allowed to be affected by a 'move2' event is `vf:onhandQuantity`, but others are, too" , "Expression does not evaluate to true" ;
                 sh:resultSeverity             sh:Violation ;
                 sh:sourceConstraint           []  ;
                 sh:sourceConstraintComponent  sh:ExpressionConstraintComponent ;
                 sh:sourceShape                vfs:EventShape_move_toResource ;
                 sh:value                      ex:event1
               ] ;
