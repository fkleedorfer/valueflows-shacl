    ]
    .    

ex:bobsAppleStash2
    a vf:EconomicResource ;
    vf:primaryAccountable ex:Alice ;
    vf:currentLocation wd:Q667328 ;
    vf:onhandQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "1200"^^xsd:int ;
    ] ;
