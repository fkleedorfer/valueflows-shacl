ex:event4
    a vf:EconomicEvent ;
    vf:action vf:move ;
    vf:provider ex:Bob ;
    vf:receiver ex:Bob ;
    vf:resourceInventoriedAs ex:bobsApples4 ;
    vf:toResourceInventoriedAs ex:bobsAppleStash ;
    vf:atLocation wd:Q667328 ;
    vf:resourceQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ; 
