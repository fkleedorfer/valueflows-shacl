ex:bobsApples4
    a vf:EconomicResource ;
    vf:previousState ex:bobsApples3 ;
    vf:primaryAccountable ex:Bob ;
    vf:currentLocation wd:Q1741 ;
    vf:onhandQuantity [
        om2:hasUnit om2:kilogram ;
