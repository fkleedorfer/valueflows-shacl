    vf:atLocation wd:Q676832 ;
    vf:resourceQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ; 
    .

ex:bobsApples3
    a vf:EconomicResource ;
    vf:previousState ex:bobsApples2 ;
    vf:primaryAccountable ex:Bob ;
    vf:currentLocation wd:Q676832 ;
    vf:onhandQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ]
    .

ex:event3
    a vf:EconomicEvent ;
    vf:action vf:move ;
    vf:provider ex:Bob ;
    vf:receiver ex:Bob ;
    vf:resourceInventoriedAs ex:bobsApples3 ;
    vf:atLocation wd:Q1741 ;
    vf:resourceQuantity [
        om2:hasUnit om2:kilogram ;
