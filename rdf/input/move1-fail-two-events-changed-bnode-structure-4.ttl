# move with only one resource
ex:Alice
    a foaf:Agent .

ex:Bob
    a foaf:Agent .

ex:bobsApples1
    a vf:EconomicResource ;
    vf:primaryAccountable ex:Bob ;
    vf:currentLocation wd:Q676832 ;
    vf:resourceClassifiedAs wd:Q4917 ;    
    vf:onhandQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ;
    vf:accountingQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ;
    ex:someProperty [
        ex:some [
            ex:funny [
                ex:structure "here" ;
            ]
        ]
    ]
    .

ex:event1
    a vf:EconomicEvent ;
    vf:action vf:move ;
    vf:provider ex:Bob ;
    vf:receiver ex:Bob ;
    vf:resourceInventoriedAs ex:bobsApples1 ;
    vf:atLocation wd:Q1741 ;
    vf:resourceQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ; 
    .

ex:bobsApples2
    a vf:EconomicResource ;
    vf:previousState ex:bobsApples1 ;
    vf:primaryAccountable ex:Bob ;   
    vf:resourceClassifiedAs wd:Q4917 ;    
    vf:currentLocation wd:Q1741 ;
    vf:onhandQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ;
    vf:accountingQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ;
    ex:someProperty [
        ex:some [
            ex:funny [
                ex:structure "here" ;  
                ex:added[                    # invalid change
                    ex:something "at a deeper level"
                ]
            ]
        ]
    ]
    .


ex:event2
    a vf:EconomicEvent ;
    vf:action vf:move ;
    vf:provider ex:Bob ;
    vf:receiver ex:Bob ;
    vf:resourceInventoriedAs ex:bobsApples2 ;
    vf:atLocation wd:Q676832 ;
    vf:resourceQuantity [
        om2:hasUnit om2:kilogram ;
        om2:hasNumericalValue "100"^^xsd:int ;
    ] ; 
    .

