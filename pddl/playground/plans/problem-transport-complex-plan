(pattern-borrow rolf claudia truck1 gedersdorf)
(pattern-travel-to-location claudia krems gedersdorf)
(pattern-taxi maria claudia car1 krems gedersdorf)
(commit-r- use maria maria car1 krems)
(begin-use maria car1)
(commit-r- mount maria claudia car1 krems)
(mount maria claudia car1 krems)
(commit-r- move maria maria car1 gedersdorf)
(move maria car1 krems gedersdorf)
(end-use maria car1)
(commit-r- dismount maria claudia car1 gedersdorf)
(dismount maria claudia car1 gedersdorf)
(commit-r- transfer-custody rolf claudia truck1 gedersdorf)
(transfer-custody rolf claudia truck1 gedersdorf)
(commit-r- use claudia claudia truck1 rossatz)
(commit-r- use claudia claudia truck1 gedersdorf)
(begin-use claudia truck1)
(pattern-travel-to-location claudia gedersdorf rossatz)
(pattern-drive claudia truck1 gedersdorf rossatz)
(commit-r- move claudia claudia truck1 rossatz)
(move claudia truck1 gedersdorf rossatz)
(commit-r- transfer-custody alice claudia apples rossatz)
(transfer-custody alice claudia apples rossatz)
(put-into claudia apples truck1)
(commit-r- move claudia claudia truck1 wien)
(move claudia truck1 rossatz wien)
(take-out-of claudia apples truck1)
(commit-r- transfer-custody claudia bob apples wien)
(transfer-custody claudia bob apples wien)
(pattern-travel-unrestricted claudia wien gedersdorf)
(pattern-drive claudia truck1 wien gedersdorf)
(commit-r- move claudia claudia truck1 gedersdorf)
(move claudia truck1 wien gedersdorf)
(end-use claudia truck1)
(commit-r- transfer-all-rights alice bob apples rossatz)
