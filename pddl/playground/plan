(send-and-transfer-recipe alice bob apples rossatz wien)
(transport-recipe claudia alice bob truck1 apples rossatz wien)
(borrow-recipe rolf claudia truck1 gedersdorf)
(taxi-recipe maria claudia car1 krems gedersdorf)
(commit use maria maria car1 krems)
(begin-use maria car1)
(commit mount maria claudia car1 krems)
(commit move maria maria car1 gedersdorf)
(commit transfer-custody claudia bob apples wien)
(commit transfer-custody rolf claudia truck1 gedersdorf)
(commit transfer-custody alice claudia apples rossatz)
(commit use claudia claudia truck1 gedersdorf)
(commit move claudia claudia truck1 wien)
(drive-recipe claudia truck1 krems rossatz)
(commit move claudia claudia truck1 rossatz)
(commit dismount maria claudia car1 gedersdorf)
(mount maria claudia car1 krems)
(move maria car1 krems gedersdorf)
(dismount maria claudia car1 gedersdorf)
(transfer-custody rolf claudia truck1 gedersdorf)
(begin-use claudia truck1)
(move claudia truck1 gedersdorf rossatz)
(transfer-custody alice claudia apples rossatz)
(drive-recipe claudia truck1 krems gedersdorf)
(commit move claudia claudia truck1 gedersdorf)
(commit transfer-custody claudia rolf truck1 gedersdorf)
(put-into claudia apples truck1)
(move claudia truck1 rossatz wien)
(take-out-of claudia apples truck1)
(transfer-custody claudia bob apples wien)
(move claudia truck1 wien gedersdorf)
(transfer-custody claudia rolf truck1 gedersdorf)
; cost = 32 (unit cost)
